{
  "presets": ["react", "es2015", "stage-2"],
  "env": {},
  "plugins": [
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }],
    ["transform-imports", {
      "react-router": {
        "transform": "react-router/${member}",
        "preventFullImport": true
      },
      "reactstrap": {
        "transform": "reactstrap/lib/${member}",
        "preventFullImport": true
      },
      "react-bootstrap": {
        "transform": "react-bootstrap/es/${member}",
        "preventFullImport": true
      }
    }]
  ]
}